%YAML 1.2
---
name: Eldiron Log
scope: source.eldironlog

contexts:
  main:
    # Error lines
    - match: (?i)^\s*\[(error|err)\].*$
      scope: keyword.other.error.log

    # Warn lines
    - match: (?i)^\s*\[(warn|warning)\].*$
      scope: keyword.other.warning.log

    # Default log lines
    - match: ^.*$
      scope: variable.other.log
